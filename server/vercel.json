{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/index.ts"
        }
    ],
    "crons": [
        {
            "path": "/api/tiktok-shop/sync/cron",
            "schedule": "0 0 * * *"
        }
    ]
}